{
    "name": "moovly/moovly",
    "description": "An integration plugin for the Moovly API",
    "type": "wordpress-plugin",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/support": "^5.3",
        "ramsey/uuid": "^3.7",
        "moovly/moovly-php-sdk": "^1.3",
        "symfony/http-foundation": "~3.4.35",
        "symfony/polyfill-mbstring": "1.20.0"
    },
    "autoload": {
        "psr-4": {
            "Moovly\\": "src/"
        }
    },
    "require-dev": {
        "giacocorsiglia/wordpress-stubs": "dev-master"
    }
}
